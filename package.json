{
  "name": "rust",
  "version": "1.0.0",
  "scripts": {
    "dev": "wrangler dev",
    "build": "cargo install -q worker-build && worker-build --release",
    "deploy": "wrangler deploy --minify",
    "fmt": "cargo fmt",
    "fmt:check": "cargo fmt --check",
    "lint": "cargo clippy",
    "lint:strict": "cargo clippy -- -D warnings",
    "check": "cargo fmt --check && cargo clippy -- -D warnings"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251217.0",
    "wrangler": "^4.55.0"
  }
}